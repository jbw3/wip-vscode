{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "WIP",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#comments"
		},
		{
			"include": "#types"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.wip",
				"match": "\\b(else|fun|if)\\b"
			}]
		},
		"comments": {
			"patterns": [{
				"name": "comment.line.wip",
				"match": "#.*"
			}]
		},
		"types": {
			"patterns": [{
				"name": "storage.type.wip",
				"match": "\\b(bool|i32)\\b"
			}]
		}
	},
	"scopeName": "source.wip"
}
